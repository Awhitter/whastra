# AgentBox V1 Environment Configuration
# Copy this file to .env and fill in your keys

# === Database ===
PG_USER=agentbox
PG_PASSWORD=your_secure_password_here
PG_DATABASE=agentbox

# === MindsDB ===
MINDSDB_PASSWORD=your_mindsdb_password
MINDSDB_URL=http://mindsdb:47334
MINDSDB_KEY=your_mindsdb_api_key

# === n8n ===
N8N_USER=admin
N8N_PASSWORD=your_n8n_password
N8N_WEBHOOK_BASE=http://n8n:5678/webhook

# === LLM Providers ===
OPENAI_API_KEY=sk-...
ANTHROPIC_API_KEY=sk-ant-...
GOOGLE_API_KEY=...
GROQ_API_KEY=gsk_...

# === Mastra Observability ===
MASTRA_CLOUD_ACCESS_TOKEN=...

# === Tools ===
APIFY_TOKEN=...
BROWSERBASE_API_KEY=...
TAVILY_API_KEY=...  # Web search

# === Optional: External Services ===
SLACK_BOT_TOKEN=xoxb-...
AIRTABLE_API_KEY=...
NOTION_API_KEY=...

# === Redis ===
REDIS_URL=redis://redis:6379

# === Qdrant ===
QDRANT_URL=http://qdrant:6333

# === Gateway ===
GATEWAY_PORT=8000
# Generate secure secret with: openssl rand -base64 32
GATEWAY_SECRET=your_gateway_secret_generate_with_openssl
